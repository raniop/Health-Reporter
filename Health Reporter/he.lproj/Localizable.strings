/* Card Explanations (info button tooltips) */
"explanation.readiness" = "כמה הגוף שלך מוכן להיום. ציון גבוה = מוכן טוב, נמוך = עדיף לנוח.";
"explanation.hrv" = "מדד שמושפע משינה ולחץ. ערך גבוה בדרך־כלל אומר שהגוף מתאושש טוב.";
"explanation.sleep" = "כמה שעות ישנת. שינה טובה עוזרת לך להרגיש טוב ולהישאר בריא.";
"explanation.strain" = "כמה עמלת על הגוף (אימונים, פעילות). חשוב לאזן עם מנוחה.";
"explanation.efficiency" = "גרף המציג את יעילות ההתאוששות שלך לאורך הזמן. כל עמודה מייצגת יום. ירוק = התאוששות טובה, צהוב = בינונית, אדום = נמוכה.";
"explanation.bioSleep" = "ציון שינה 0–100 המשקלל משך שינה ואיכות (שינה עמוקה + REM). 80+ = מצוין, 60-80 = טוב, מתחת ל-60 = דורש שיפור.";
"explanation.bioTemp" = "הפרש טמפרטורת הגוף מהרגיל. שינוי גדול יכול להראות עייפות או חולי.";
"explanation.bioRhrOrTemp" = "דופק מנוחה (RHR) - מדד חשוב לכושר והתאוששות. ככל שנמוך יותר, הלב יעיל יותר. טווח בריא: 50-70 פעימות לדקה.";
"explanation.directives" = "הנחיות AI מבוססות על הנתונים שלך. עצור = הרגלים להפסיק. התחל = פעולות מומלצות. עקוב = דברים לשים לב אליהם.";
"explanation.insight" = "תובנה מהנתונים שלך – קשר בין דברים (למשל שינה ומדדים).";
"explanation.correlation" = "מדד קורלציה: עד כמה שני מדדים קשורים. 1.0 = קשר חזק חיובי, -1.0 = קשר הפוך, 0 = אין קשר.";
"explanation.pValue" = "P-Value - מדד סטטיסטי לאמינות. מתחת ל-0.05 = הקשר כנראה אמיתי ולא מקרי. ככל שנמוך יותר, כך אמין יותר.";
"explanation.sampleSize" = "מספר הימים שעליהם מבוססים הנתונים. 30+ ימים = תובנות אמינות יותר.";
"explanation.biometrics" = "סיכום מדדים ביומטריים: דופק מנוחה (RHR), שונות דופק (HRV), ועומס פיזי - להערכת מצב הבריאות.";
"explanation.focus" = "מוקדי התמקדות - תחומים שזוהו כחשובים לשיפור, מבוססים על ניתוח הנתונים שלך.";
"explanation.profileHeight" = "הגובה שלך מאפליקציית הבריאות של אפל. משמש לחישוב BMI ומדדים נוספים.";
"explanation.profileWeight" = "המשקל העדכני שלך מאפליקציית הבריאות. עדכון שוטף מאפשר מעקב מדויק.";
"explanation.highlights" = "דגשים יומיים: זמן במיטה, קצב נשימות בשינה, צעדים ומרחק - להבנת הפעילות היומית שלך.";
"explanation.profileAge" = "הגיל שלך מתאריך הלידה. משמש לכיול המלצות בריאות מותאמות אישית.";
"explanation.activitySteps" = "מספר הצעדים שביצעת. יעד מומלץ: 10,000 ביום. הליכה סדירה משפרת בריאות הלב ומצב רוח.";
"explanation.activityDistance" = "סה״כ המרחק שעברת ברגל (הליכה + ריצה). יעד יומי מומלץ: 5-8 ק״מ.";
"explanation.activityCalories" = "קלוריות פעילות - אנרגיה שנשרפת מתנועה ואימון (לא כולל חילוף חומרים בסיסי). יעד: 300-500 ביום.";
"explanation.activityExercise" = "דקות פעילות בעצימות בינונית-גבוהה. המלצת WHO: 150+ דקות בשבוע.";
"explanation.activityFlights" = "מספר קומות מדרגות שעלית. טיפוס מדרגות שורף קלוריות במהירות כפולה מהליכה.";
"explanation.activityMove" = "דקות שהיית פעיל. המטרה: להישאר פעיל לאורך היום, לא רק באימון.";
"explanation.activityStand" = "שעות שעמדת וזוזת לפחות דקה. יעד: 12 שעות ביום. ישיבה ממושכת מזיקה.";
"explanation.activityCycling" = "מרחק רכיבת אופניים. פעילות אירובית מצוינת שחוסכת עומס על המפרקים.";
"explanation.activitySwimming" = "מרחק שחייה. אימון גוף מלא שמשפר סיבולת ומחזק שרירים.";

/* Hero Card Mini KPI Explanations */
"explanation.heroSleep" = "משך השינה הממוצע שלך בתקופה הנבחרת.\n\nשינה איכותית היא הבסיס להתאוששות הגוף. מבוגרים צריכים 7-9 שעות שינה בלילה.\n\nטיפים לשיפור:\n• שמור על שעת שינה קבועה\n• הימנע ממסכים שעה לפני השינה\n• שמור על חדר חשוך וקריר";
"explanation.heroHRV" = "שונות קצב הלב (Heart Rate Variability) - מדד חשוב להתאוששות ולחץ.\n\nHRV גבוה מעיד על מערכת עצבים גמישה ויכולת התאוששות טובה. HRV נמוך עשוי להצביע על עייפות, מתח או מחלה.\n\nערכים משתנים בין אנשים - חשוב לעקוב אחר המגמה האישית שלך לאורך זמן.";
"explanation.heroStrain" = "ציון העומס הפיזי שלך - כמה ״עבדת״ את הגוף.\n\nעומס גבוה אומר שהגוף עבד קשה (אימונים, פעילות). עומס נמוך מראה על יום נינוח יותר.\n\nאיזון חשוב: ימי עומס גבוה צריכים להיות מאוזנים עם ימי התאוששות לשיפור הביצועים והבריאות.";

